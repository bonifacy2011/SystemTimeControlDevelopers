<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">

	<display-name>SystemTimeControlDevelopers</display-name>


	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>WelcomeAction</servlet-name>
		<servlet-class>com.timemanager.actions.WelcomeAction</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>WelcomeAction</servlet-name>
		<url-pattern>/welcome.do</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>ResponsibleAction</servlet-name>
		<servlet-class>com.timemanager.actions.ResponsiblesAction</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ResponsibleAction</servlet-name>
		<url-pattern>/responsibles.do</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>RecordingElapsedTimeAction</servlet-name>
		<servlet-class>com.timemanager.actions.RecordingElapsedTimeAction</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>RecordingElapsedTimeAction</servlet-name>
		<url-pattern>/recording_elapsed_time.do</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>TaskAction</servlet-name>
		<servlet-class>com.timemanager.actions.TaskAction</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>TaskAction</servlet-name>
		<url-pattern>/task.do</url-pattern>
	</servlet-mapping>
</web-app>